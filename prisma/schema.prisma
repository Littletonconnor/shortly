datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Shortner {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  url       String
  slug      String   @unique
}
